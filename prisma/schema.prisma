generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Member {
  id            String   @id @default(uuid())
  firstName     String
  lastName      String
  email         String   @unique
  contactNumber String
  designation   String
  signature     Boolean  @default(true)
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
